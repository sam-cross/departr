{
  "name": "rail-times-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fuzzysearch": "^1.0.3",
    "leaflet": "^1.6.0",
    "modern-normalize": "^0.6.0",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-leaflet": "^2.6.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "soap": "^0.29.0",
    "styled-components": "^5.0.1",
    "xml2js": "^0.4.23"
  },
  "scripts": {
    "serve": "concurrently --kill-others-on-fail \"yarn start\" \"yarn server\"",
    "server": "yarn server-db&&nodemon",
    "server-ts": "node --inspect=6660 -r ts-node/register ./api/index.js",
    "server-db": "powershell -Command \"Start-Process -FilePath 'net' -ArgumentList 'start MongoDB' -Verb runAs\"",
    "populate-db": "yarn server-db&&node ./api/db/populate.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "api"
    ],
    "exec": "yarn server-ts",
    "ext": "js,ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^13.9.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}